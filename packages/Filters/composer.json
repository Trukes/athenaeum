{
    "name": "aedart/athenaeum-filters",
    "description": "Search filter utilities, based on http query parameters",
    "keywords": ["Athenaeum", "Search", "Filters", "Http", "API"],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "aedart/athenaeum-contracts": "^5.27",
        "aedart/athenaeum-database": "^5.27",
        "aedart/athenaeum-support": "^5.27",
        "aedart/athenaeum-utils": "^5.27",
        "aedart/athenaeum-validation": "^5.27",
        "illuminate/http": "v9.0.1",
        "illuminate/pipeline": "v9.0.1"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Filters\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.27.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
