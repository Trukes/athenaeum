{
    "name": "aedart/athenaeum-database",
    "description": "Database utilities for Laravel",
    "keywords": ["Athenaeum", "Database", "Db"],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "aedart/athenaeum-contracts": "^5.27",
        "aedart/athenaeum-support": "^5.27",
        "aedart/athenaeum-utils": "^5.27",
        "illuminate/database": "^v9.0.0-beta.5"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Database\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.27.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
