{
    "name": "aedart/athenaeum-redmine",
    "description": "Redmine API Client for Laravel",
    "keywords": ["Athenaeum", "Redmine", "API"],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "ext-json": "*",
        "aedart/athenaeum-dto": "^5.27",
        "aedart/athenaeum-pagination": "^5.28",
        "aedart/athenaeum-http-clients": "^5.27",
        "aedart/athenaeum-dto": "^5.28"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Redmine\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.28.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
