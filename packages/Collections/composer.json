{
    "name": "aedart/athenaeum-collections",
    "description": "Collections and data processing utilities",
    "keywords": ["Athenaeum", "Collections", "Items Processor"],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "aedart/athenaeum-container": "^5.27",
        "aedart/athenaeum-contracts": "^5.27",
        "aedart/athenaeum-support": "^5.27",
        "aedart/athenaeum-utils": "^5.27",
        "illuminate/collections": "^v9.0.0-beta.5"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Collections\\": "src"
        }
    },
    "suggest": {
        "symfony/var-dumper": "symfony/var-dumper: Required to use the dd and dump functions."
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.27.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
