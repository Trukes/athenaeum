{
    "name": "aedart/athenaeum-acl",
    "description": "ACL package for Laravel, with roles and permissions (grouped)",
    "keywords": ["Athenaeum", "Acl", "Access Controll List", "Roles", "Permissions", "Authorization"],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "aedart/athenaeum-contracts": "^5.27",
        "aedart/athenaeum-database": "^5.27",
        "aedart/athenaeum-support": "^5.27",
        "aedart/athenaeum-utils": "^5.27",
        "illuminate/auth": "^8.15"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Acl\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.27.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
