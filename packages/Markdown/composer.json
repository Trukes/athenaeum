{
    "name": "aedart/athenaeum-markdown",
    "description": "Athenaeum markdown package",
    "keywords": [
        "Athenaeum",
        "Markdown"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "support": {
        "issues": "https://github.com/aedart/athenaeum/issues",
        "source": "https://github.com/aedart/athenaeum"
    },
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "aedart/athenaeum-support": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Markdown\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "5.7.x-dev"
        }
    }
}
