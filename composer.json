{
  "name": "aedart/athenaeum",
  "description": "Athenaeum is collection of various library components that are built using several Laravel components",
  "keywords": [
    "Library",
    "Testing",
    "Utilities",
    "Test Cases",
    "Overload",
    "Magic Method"
  ],
  "homepage": "https://aedart.github.io/athenaeum/",
  "support": {
    "issues": "https://github.com/aedart/athenaeum/issues",
    "source": "https://github.com/aedart/athenaeum"
  },
  "license": "BSD-3-Clause",
  "type": "library",
  "authors": [
    {
      "name": "Alin Eugen Deac",
      "email": "aedart@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Aedart\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aedart\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.2.0",
    "fzaninotto/faker": "1.8.*",
    "mockery/mockery": "1.1.*",
    "illuminate/support": "5.7.*"
  },
  "require-dev": {
    "codeception/codeception": "2.4.*",
    "symfony/var-dumper": "4.1.*"
  },
  "suggest": {
    "codeception/codeception": "Required for Testing utilities"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
