{
    "name": "aedart/athenaeum",
    "description": "Athenaeum is a mono repository; a collection of various PHP packages",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "ext-json": "*",
        "composer-runtime-api": "^2.0.0",
        "dragonmantank/cron-expression": "^3.3",
        "fakerphp/faker": "^v1.17.0",
        "guzzlehttp/guzzle": "^7.4.1",
        "illuminate/auth": "^v9.0.0-beta.5",
        "illuminate/cache": "^v9.0.0-beta.5",
        "illuminate/collections": "^v9.0.0-beta.5",
        "illuminate/config": "^v9.0.0-beta.5",
        "illuminate/console": "^v9.0.0-beta.5",
        "illuminate/container": "^v9.0.0-beta.5",
        "illuminate/contracts": "^v9.0.0-beta.5",
        "illuminate/database": "^v9.0.0-beta.5",
        "illuminate/events": "^v9.0.0-beta.5",
        "illuminate/filesystem": "^v9.0.0-beta.5",
        "illuminate/http": "^v9.0.0-beta.5",
        "illuminate/pipeline": "^v9.0.0-beta.5",
        "illuminate/support": "^v9.0.0-beta.5",
        "illuminate/validation": "^v9.0.0-beta.5",
        "mockery/mockery": "1.5.*",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/log": "^1.1",
        "shrikeh/teapot": "^2.3",
        "symfony/console": "^v6.0.2",
        "vlucas/phpdotenv": "^5.1"
    },
    "require-dev": {
        "ext-curl": "*",
        "ext-sockets": "*",
        "bamarni/composer-bin-plugin": "^1.4",
        "codeception/codeception": "5.0.0-alpha1",
        "codeception/module-asserts": "*@dev",
        "phpunit/phpunit": "dev-master as 9.5.99",
        "orchestra/testbench": "7.x-dev",
        "orchestra/testbench-dusk": "7.x-dev",
        "predis/predis": "^1.1.4",
        "roave/security-advisories": "dev-master",
        "symfony/var-dumper": "^v6.0.2",
        "symfony/yaml": "^v6.0.2",
        "twig/twig": "^3.3.7",
        "yosymfony/toml": "^1.0.4"
    },
    "autoload": {
        "files": [
            "packages/Core/helpers/paths.php",
            "packages/Support/helpers/properties.php"
        ],
        "psr-4": {
            "Aedart\\": "src",
            "Aedart\\Acl\\": "packages/Acl/src",
            "Aedart\\Audit\\": "packages/Audit/src",
            "Aedart\\Circuits\\": "packages/Circuits/src",
            "Aedart\\Collections\\": "packages/Collections/src",
            "Aedart\\Config\\": "packages/Config/src",
            "Aedart\\Console\\": "packages/Console/src",
            "Aedart\\Container\\": "packages/Container/src",
            "Aedart\\Contracts\\": "packages/Contracts/src",
            "Aedart\\Core\\": "packages/Core/src",
            "Aedart\\Database\\": "packages/Database/src",
            "Aedart\\Dto\\": "packages/Dto/src",
            "Aedart\\Events\\": "packages/Events/src",
            "Aedart\\Filters\\": "packages/Filters/src",
            "Aedart\\Http\\Clients\\": "packages/Http/Clients/src",
            "Aedart\\Http\\Cookies\\": "packages/Http/Cookies/src",
            "Aedart\\Http\\Messages\\": "packages/Http/Messages/src",
            "Aedart\\Maintenance\\Modes\\": "packages/Maintenance/Modes/src",
            "Aedart\\Pagination\\": "packages/Pagination/src",
            "Aedart\\Properties\\": "packages/Properties/src",
            "Aedart\\Redmine\\": "packages/Redmine/src",
            "Aedart\\Service\\": "packages/Service/src",
            "Aedart\\Support\\": "packages/Support/src",
            "Aedart\\Testing\\": "packages/Testing/src",
            "Aedart\\Utils\\": "packages/Utils/src",
            "Aedart\\Validation\\": "packages/Validation/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aedart\\Tests\\": "tests/"
        }
    },
    "replace": {
        "aedart/athenaeum-acl": "self.version",
        "aedart/athenaeum-audit": "self.version",
        "aedart/athenaeum-circuits": "self.version",
        "aedart/athenaeum-collections": "self.version",
        "aedart/athenaeum-config": "self.version",
        "aedart/athenaeum-console": "self.version",
        "aedart/athenaeum-container": "self.version",
        "aedart/athenaeum-contracts": "self.version",
        "aedart/athenaeum-core": "self.version",
        "aedart/athenaeum-database": "self.version",
        "aedart/athenaeum-dto": "self.version",
        "aedart/athenaeum-events": "self.version",
        "aedart/athenaeum-filters": "self.version",
        "aedart/athenaeum-http-clients": "self.version",
        "aedart/athenaeum-http-cookies": "self.version",
        "aedart/athenaeum-http-messages": "self.version",
        "aedart/athenaeum-maintenance-modes": "self.version",
        "aedart/athenaeum-pagination": "self.version",
        "aedart/athenaeum-properties": "self.version",
        "aedart/athenaeum-redmine": "self.version",
        "aedart/athenaeum-service": "self.version",
        "aedart/athenaeum-support": "self.version",
        "aedart/athenaeum-testing": "self.version",
        "aedart/athenaeum-utils": "self.version",
        "aedart/athenaeum-validation": "self.version"
    },
    "scripts": {
        "post-install-cmd": [
            "@test-env",
            "@update-bin"
        ],
        "post-update-cmd": [
            "@update-bin"
        ],
        "test-env": [
            "@php -r \"file_exists('.testing') || copy('.testing.example', '.testing');\""
        ],
        "update-bin": [
            "@composer bin all update --ansi"
        ],
        "check": [
            "vendor/bin/phpcs -p --standard=PHPCompatibility --colors --report-full --report-summary --runtime-set testVersion 8.0- ./packages ./tests"
        ],
        "cs": "vendor/bin/ecs check --ansi",
        "cs-fix": "vendor/bin/ecs check --fix --ansi",
        "docs:dev": "npm run docs:dev --ansi"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.27.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
