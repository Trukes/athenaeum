{
    "name": "aedart/athenaeum",
    "description": "Athenaeum is collection of various library components that are built using several Laravel components",
    "keywords": [
        "Library",
        "Utilities",
        "Testing",
        "Overloading",
        "Magic Methods",
        "DTO",
        "Configuration"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "support": {
        "issues": "https://github.com/aedart/athenaeum/issues",
        "source": "https://github.com/aedart/athenaeum"
    },
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Aedart\\": "src/"
        },
        "files": [
            "helpers/dto-properties.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Aedart\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4.0",
        "ext-json": "*",
        "fzaninotto/faker": "1.9.*",
        "mockery/mockery": "1.3.*",
        "illuminate/support": "^6.8",
        "illuminate/config": "^6.8",
        "illuminate/container": "^6.8",
        "illuminate/filesystem": "^6.8",
        "illuminate/events": "^6.8",
        "symfony/console": "4.4.*",
        "jean85/pretty-package-versions": "~1.2",
        "psr/http-message": "^1.0.1",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "ext-sockets": "*",
        "orchestra/testbench": "^4.4",
        "codeception/codeception": "3.1.*",
        "symfony/var-dumper": "4.4.*",
        "predis/predis": "~1.1",
        "symfony/yaml": "4.4.*",
        "twig/twig": "^2.11",
        "hanneskod/classtools": "~1.2",
        "guzzlehttp/guzzle": "~6.4"
    },
    "suggest": {
        "codeception/codeception": "Required for Testing utilities",
        "orchestra/testbench": "Required for Laravel Testing utilities",
        "symfony/yaml": "Required for parsing Yaml configuration files",
        "twig/twig": "Required for using aware-of generator"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "athenaeum"
    ],
    "scripts": {
        "post-install-cmd": [
            "@test-env"
        ],
        "test-env": [
            "@php -r \"file_exists('.testing') || copy('.testing.example', '.testing');\""
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
